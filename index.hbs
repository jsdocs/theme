<div class='clearfix md-mxn2'>
  <div class='fixed md-show fix-3 overflow-auto max-height-100'>
    <div class='py1 px2'>
      <h3 class='mb0 no-anchor'>{{options.pkg.name}}</h3>
      <div class='mb1'><code>{{options.pkg.version}}</code></div>
      <br />
      <input
        placeholder='Filter'
        id='filter-input'
        class='col12 block field'
        type='text' />
      <div id='toc'>
        {{#each docs}}
          <a
            href='#{{permalink}}'
            class='block bold'>
            {{name}}
          </a>
          {{#each members.static}}
            <a
              href='#{{permalink}}'
              class='regular block'>
              .{{name}}
            </a>
          {{/each}}
          {{#each members.instance}}
            <a
              href='#{{permalink}}'
              class='regular block nested'>
              #{{name}}
            </a>
          {{/each}}
          {{#each events}}
            <a
              href='#{{permalink}}'
              class='regular block nested'>
              <code>â“” #{{name}}</code>
            </a>
          {{/each}}
        {{/each}}
      </div>
    </div>
  </div>
  <div class='fix-margin-3'>
    <div class='px2'>
{{#each docs ~}}<div class='py1'>{{> section}}</div>{{/each}}
    </div>
  </div>
</div>
